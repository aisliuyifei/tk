<p id="notice"><%= notice %></p>

<h1>Listing Pics</h1>

<table>
  <thead>
    <tr>
      <th>Gallery</th>
      <th>Original url</th>
      <th>Oss url</th>
      <th>Code</th>
      <th>Visit count</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pics.each do |pic| %>
      <tr>
        <td><%= pic.gallery_id %></td>
        <td><%= pic.original_url %></td>
        <td><%= pic.oss_url %></td>
        <td><%= pic.code %></td>
        <td><%= pic.visit_count %></td>
        <td><%= link_to 'Show', pic %></td>
        <td><%= link_to 'Edit', edit_pic_path(pic) %></td>
        <td><%= link_to 'Destroy', pic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pic', new_pic_path %>
